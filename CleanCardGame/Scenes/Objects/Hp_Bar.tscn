func _process(delta):
	
	curr_hp = get_node(\"TextureProgress\").get_value()
	get_node(\"TextureProgress\").set_value(curr_hp - (int(10)*delta))
	
	if  curr_hp <= new_val:
		set_process(false)
	pass

func update_hp(hp):
	set_process(true)
	new_val = hp
	pass

func create_hp(max_hp):
	get_node(\"TextureProgress\").set_max(max_hp)
	get_node(\"TextureProgress\").set_value(max_hp)
	pass

